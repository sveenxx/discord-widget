{
  "name": "discord-widget",
  "version": "1.1.2",
  "description": "♻️ Customize discord widget for your website",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "author": "Jakub Szołtysek",
  "keywords": [
    "discord",
    "widget",
    "custom"
  ],
  "targets": {
    "main": {
      "outputFormat": "esmodule"
    },
    "browser": {
      "includeNodeModules": true
    }
  },
  "license": "MIT",
  "scripts": {
    "dev": "parcel src/index.ts index.html",
    "build": "parcel build src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sveenxx/discord-widget.git"
  },
  "devDependencies": {
    "@parcel/packager-ts": "2.0.0-rc.0",
    "@parcel/transformer-typescript-types": "2.0.0-rc.0",
    "parcel": "^2.0.0-rc.0",
    "typescript": ">=3.0.0"
  }
}
